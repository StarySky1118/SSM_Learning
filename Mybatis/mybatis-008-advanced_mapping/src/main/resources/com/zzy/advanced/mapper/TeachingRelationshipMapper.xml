<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zzy.advanced.mapper.TeachingRelationshipMapper">
    <select id="queryTeachingRelationshipsBySid" resultType="TeachingRelationship">
        select id, sid, tid
        from t_teachingrelation
        where sid = #{sid}
    </select>
</mapper>